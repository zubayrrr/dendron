wisted pair","id":"q6rcmbb4k2on7oh9zfm90ba","vaultName":"Dendron"},"value":"devlog.Straight-Through Patch Cable","alias":"devlog.Straight-Through Patch Cable","position":{"start":{"line":23,"column":12,"offset":796},"end":{"line":23,"column":51,"offset":835},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"devlog.Straight-Through Patch Cable"}},{"type":"wiki","from":{"fname":"devlog.twisted pair","id":"q6rcmbb4k2on7oh9zfm90ba","vaultName":"Dendron"},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair","position":{"start":{"line":84,"column":11,"offset":2045},"end":{"line":84,"column":34,"offset":2068},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"devlog.Twisted Pair"}},{"type":"wiki","from":{"fname":"devlog.twisted pair","id":"q6rcmbb4k2on7oh9zfm90ba","vaultName":"Dendron"},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair","position":{"start":{"line":94,"column":11,"offset":2245},"end":{"line":94,"column":34,"offset":2268},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"devlog.Twisted Pair"}},{"from":{"fname":"devlog.connector types","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":22,"column":49,"offset":740},"end":{"line":22,"column":72,"offset":763},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.ethernet","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":10,"column":33,"offset":409},"end":{"line":10,"column":56,"offset":432},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.fiber optic vs copper","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":9,"column":27,"offset":155},"end":{"line":9,"column":50,"offset":178},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.fiber optic","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":6,"column":65,"offset":101},"end":{"line":6,"column":88,"offset":124},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.media and cabling distribution","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":5,"column":3,"offset":59},"end":{"line":5,"column":26,"offset":82},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.media","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":7,"column":3,"offset":241},"end":{"line":7,"column":26,"offset":264},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.network cables","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":2,"column":3,"offset":3},"end":{"line":2,"column":26,"offset":26},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.network interface layer - tcpip","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":7,"column":49,"offset":266},"end":{"line":7,"column":72,"offset":289},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.rj-11","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":3,"column":11,"offset":29},"end":{"line":3,"column":34,"offset":52},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.rj-45","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":4,"column":11,"offset":82},"end":{"line":4,"column":34,"offset":105},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.twisted pair","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":84,"column":11,"offset":2045},"end":{"line":84,"column":34,"offset":2068},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"},{"from":{"fname":"devlog.twisted pair","vaultName":"Dendron"},"type":"backlink","position":{"start":{"line":94,"column":11,"offset":2245},"end":{"line":94,"column":34,"offset":2268},"indent":[]},"value":"devlog.Twisted Pair","alias":"devlog.Twisted Pair"}],"anchors":{"t-568a-wiring-standard":{"type":"header","text":"T-568A wiring standard","value":"t-568a-wiring-standard","line":22,"column":0,"depth":4},"t-568b-wiring-standard":{"type":"header","text":"T-568B wiring standard","value":"t-568b-wiring-standard","line":27,"column":0,"depth":4},"twisted-pair-cable-throughput":{"type":"header","text":"Twisted Pair cable throughput","value":"twisted-pair-cable-throughput","line":34,"column":0,"depth":3},"type-of-twisted-pair-connectors":{"type":"header","text":"Type of Twisted Pair Connectors","value":"type-of-twisted-pair-connectors","line":82,"column":0,"depth":3}},"children":[],"parent":"9gtn7g40cvqui0sifl1s7t5","data":{}},"body":"<h1 id=\"twisted-pair\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#twisted-pair\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Twisted Pair</h1>\n<ul>\n<li>Areas: <a href=\"/notes/owoutsv5dicylguol2odc3e\">Linux</a></li>\n</ul>\n<hr>\n<ul>\n<li>Transmits electrons</li>\n<li>Twists reduces interference</li>\n<li>Most popular physical LAN media type</li>\n<li>Eight individually insulated strands of copper wire inside each cable</li>\n<li>Types:\n<ul>\n<li>Shielded(<a href=\"/notes/mfu7kh5sjdof5wk03rf0pl0\">STP</a>) gives additional insulating barrier against electronic magnetic interference (beneficial where theres a lot of electrical noise)</li>\n<li>Unshielded (<a href=\"/notes/2xucu5yaqtnegnuf5p40j43\">UTP</a>) doesn't have extra protection but is useful in most standard office environments and is also cheaper.</li>\n</ul>\n</li>\n</ul>\n<hr>\n<h4 id=\"t-568a-wiring-standard\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#t-568a-wiring-standard\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>T-568A wiring standard</h4>\n<ul>\n<li>Often used for <a title=\"Private\" style=\"color: brown\" href=\"https://wiki.dendron.so/notes/hfyvYGJZQiUwQaaxQO27q.html\" target=\"_blank\">Crossover Cable (Private)</a>, used to link older <a href=\"/notes/co4eno6fzx5ov2408cmwe3w\">Ethernet</a> netowork switches, although modern switches can detect what type of cable is plugged in.</li>\n<li>Only one end of the cable is made using this standard.</li>\n</ul>\n<h4 id=\"t-568b-wiring-standard\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#t-568b-wiring-standard\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>T-568B wiring standard</h4>\n<ul>\n<li>Used for <a href=\"/notes/ebbsmmds8qkj39bw5g14klp\">Straight-Through Patch Cable</a>.</li>\n<li>Both end of the cables are twisted using this standard.</li>\n</ul>\n<hr>\n<h3 id=\"twisted-pair-cable-throughput\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#twisted-pair-cable-throughput\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Twisted Pair cable throughput</h3>\n<table>\n<thead>\n<tr class=\"header\">\n<th>Category</th>\n<th>Max Throughput</th>\n<th style=\"text-align: right;\">Max Distance</th>\n</tr>\n</thead>\n<tbody>\n<tr class=\"odd\">\n<td>Cat 3</td>\n<td>10 Mbps</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n<tr class=\"even\">\n<td>Cat 5</td>\n<td>100 Mbps</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n<tr class=\"odd\">\n<td>Cat 5e</td>\n<td>1,000 Mbps (1 Gbps)</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n<tr class=\"even\">\n<td>Cat 6</td>\n<td>1,000 Mbps (1 Gbps)</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n<tr class=\"odd\">\n<td>Cat 6a</td>\n<td>10,000 Mbps (10 Gbps)</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n<tr class=\"even\">\n<td>Cat 7</td>\n<td>100,000 Mbps (10 Gbps)</td>\n<td style=\"text-align: right;\">100 meters</td>\n</tr>\n</tbody>\n</table>\n<p>The thicker the copper the better, the more twists of the adjacent wires per inch to reduce crosstalk interference.</p>\n<hr>\n<h3 id=\"type-of-twisted-pair-connectors\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#type-of-twisted-pair-connectors\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Type of Twisted Pair Connectors</h3>\n<ul>\n<li>RJ-45:</li>\n</ul>\n<!-- end list -->\n<ul>\n<li>8-pin connector in Ethernet networks</li>\n<li>Most Ethernet use only 4-pins</li>\n<li>It is a <a href=\"/notes/q6rcmbb4k2on7oh9zfm90ba\">Twisted Pair</a> cable connector.</li>\n<li>You can also use it for token ring network if you've the appropriate equipment.</li>\n</ul>\n<!-- end list -->\n<ul>\n<li>RJ-11:</li>\n</ul>\n<!-- end list -->\n<ul>\n<li>6-pin connector</li>\n<li>It is a <a href=\"/notes/q6rcmbb4k2on7oh9zfm90ba\">Twisted Pair</a> cable connector</li>\n<li>Commonly only 2 or 4 pins are used</li>\n<li>Commonly found in telephone systems</li>\n<li>A basic telephone setup really requires only two wires</li>\n</ul>\n<hr>\n<strong>Backlinks</strong>\n<ul>\n<li><a href=\"/notes/fzlykicsgkocfcx21dc9st2\">Connector Types</a></li>\n<li><a href=\"/notes/co4eno6fzx5ov2408cmwe3w\">Ethernet</a></li>\n<li><a href=\"/notes/ckp7breg1zjim1bhjjj8agf\">Fiber Optic vs Copper</a></li>\n<li><a href=\"/notes/y7dwtr5ud15dkh6e7x7ose4\">Fiber Optic</a></li>\n<li><a href=\"/notes/ogr2uw13q0pto3dkfy5bbxd\">Media and Cabling Distribution</a></li>\n<li><a href=\"/notes/cmj4t357d9m7uwpuo9k0ed2\">Media</a></li>\n<li><a href=\"/notes/c56omihefzozvm01zqiihlu\">Network Cables</a></li>\n<li><a href=\"/notes/h9fwgm5qmlznj6jloqqqlfl\">Network Interface Layer - TCP/IP</a></li>\n<li><a href=\"/notes/1s63w9j32u2k4cc6m231dai\">RJ-11</a></li>\n<li><a href=\"/notes/ybp1ugwoj4sywjxyk876ql1\">RJ-45</a></li>\n<li><a href=\"/notes/q6rcmbb4k2on7oh9zfm90ba\">Twisted Pair</a></li>\n</ul>","noteIndex":{"id":"3nfl4nvv516muyzozhcwrw8","title":"/root","desc":"","updated":1655559901157,"creat